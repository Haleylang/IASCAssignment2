define(function(require){var $=require("jquery"),videoHeroModule=require("video-hero-module");var dom={};function cacheDom(){dom={};dom.heroVideo=$(".hero-wrapper .individual_hero_video");dom.heroV2Video=$(".hero-wrapper-v2 .individual_hero_video")}function init(){cacheDom();createVideo()}function createVideo(){dom.heroVideo.each(function(index,el){var videoElement=$(el),heroWrapper=videoElement.closest(".hero-wrapper");videoHeroModule.create({heroWrapper:heroWrapper})});dom.heroV2Video.each(function(index,el){var videoElement=$(el),heroWrapperV2=videoElement.closest(".hero-wrapper-v2");videoHeroModule.create({heroWrapper:heroWrapperV2})})}init()});