define("condition-lightbox-module",function(require){"use strict";require("array-from");var $=require("jquery"),MicroModal=require("micromodal"),conditionOverlay=require("condition-overlay"),model={};create();return{initialize:initialize,show:show};function create(){model.$body=$("body");model.$conditionSnippet=$(".condition-snippet-wrapper")}function initialize(){MicroModal.init()}function show(){model.$body.addClass("lightbox-open");MicroModal.show("conditions-overlay",{onShow:onShow,onClose:onClose})}function onClose(){model.$body.removeClass("lightbox-open");model.$conditionSnippet.removeClass("opened")}function onShow(){if(!conditionOverlay.carouselInitalized()){return}conditionOverlay.refreshCarousel();conditionOverlay.attachEventHandlers()}});